    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@svgdotjs/svg.js": "^3.2.4",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "element-plus": "^2.9.3",
    "file-saver": "2.0.5",
