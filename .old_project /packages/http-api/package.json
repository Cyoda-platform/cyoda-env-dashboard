    "js-beautify": "^1.15.1",
    "lodash-unified": "^1.0.3",
    "modern-normalize": "3.0.1",
    "moment": "^2.30.1",
    "portal-vue": "^3.0.0",
    "prismjs": "1.29.0",
    "qs": "^6.14.0",
    "vue": "^3.5.13",
    "vue-filepond": "^7.0.4",
    "vue-google-charts": "^1.1.0",
    "vue-router": "^4.5.0",
    "vuedraggable": "^4.1.0"
